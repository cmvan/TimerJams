{
    "name": "TimerJams",
    "author": "Caroline Van",
    "manifest_version": 3,
    "version": "1.0",
    "description": "Stay productive with Pomodoro-based timers and a space to play your own study music!",
    "permissions": ["activeTab", "tabs"],
    "chrome_url_overrides": {
        "newtab": "intro.html"
    },
    "action": {
        "default_icon": "media/clock16.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "study.js"
    }
}